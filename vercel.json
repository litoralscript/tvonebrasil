{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.5"
    },
    "*.php": {
      "runtime": "vercel-php@0.7.5"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/config/(.*)",
      "status": 403
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
